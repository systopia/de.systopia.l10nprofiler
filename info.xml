<?xml version="1.0"?>
<extension key="de.systopia.l10nprofiler" type="module">
  <file>l10nprofiler</file>
  <name>Localisation Profiler</name>
  <description>Allows detailed profiling of translated strings</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>SYSTOPIA</author>
    <email>endres@systopia.de</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/systopia/de.systopia.l10nprofiler</url>
    <url desc="Documentation">https://github.com/systopia/de.systopia.l10nprofiler</url>
    <url desc="Support">https://github.com/systopia/de.systopia.l10nprofiler/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate/>
  <version>0.1-dev</version>
  <develStage>dev</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <requires>
    <ext>org.civicrm.l10nx</ext>
  </requires>
  <comments/>
  <civix>
    <namespace>CRM/L10nprofiler</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_L10nprofiler_Upgrader</upgrader>
</extension>
